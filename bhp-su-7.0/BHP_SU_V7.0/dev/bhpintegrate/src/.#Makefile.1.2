include ../../Makefile_template

RCSfiles.c = \
	bhpintegrate.c
	
RCSfiles.f = \

RCSfiles = $(RCSfiles.c)  $(RCSfiles.f)

EXEROOT = bin/$(PLATFORMID)/bhpintegrate
EXE = ../../$(EXEROOT)

OBJS.c = $(RCSfiles.c:.c=.o)
OBJS.f = $(RCSfiles.f:.f=.o)
OBJS   = $(OBJS.c) $(OBJS.f)

all: $(EXE)

depend:
	makedepend $(PKG_INC.c) $(RCSfiles.c)
	
$(EXE):  $(OBJS)  $(PKG_ARCHIVE)
	$(RM) $(EXE)
	$(LINK.c) -o $@ $? $(PKG_LIB) $(LDFLAG)
	chmod 755 $(EXE)

clobber:
	$(RM)  *.o
	$(RM)  $(EXE)
	$(RM)  Makefile.bak
	
clean:
	$(RM)  *.o
	$(RM)  Makefile.bak
	
beta: $(EXE)
	cp $(EXE)  ../../../beta/$(EXEROOT)
	chmod 755 ../../../beta/$(EXEROOT)
	
prod:
	cp ../../../beta/$(EXEROOT) ../../../prod/$(EXEROOT)
	chmod 755 ../../../prod/$(EXEROOT)
	
